{
  "name": "story-submission-api",
  "version": "1.0.0",
  "description": "Simple express backend to send e-mails and files",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "nodemon",
    "db:generate": "npx prisma generate",
    "db:migration-generate": "npx prisma migrate dev --name",
    "db:seed": "ts-node src/prisma/seeders/index.ts",
    "db:migration": "npx prisma migrate dev",
    "db:update": "npm run db:migration && npm run db:seed && npm run db:generate"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.3.1",
    "ts-node": "^10.9.1",
    "typescript": "*"
  },
  "dependencies": {
    "@google-cloud/local-auth": "2.1.0",
    "@prisma/client": "^4.3.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "googleapis": "105",
    "helmet": "^6.0.0",
    "http-status": "^1.5.3",
    "morgan": "^1.10.0",
    "yup": "^0.32.11"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
